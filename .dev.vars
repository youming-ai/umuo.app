# OpenNext.js Cloudflare Workers 开发环境变量
# 本地开发和预览环境使用

NODE_ENV=development
NEXT_PUBLIC_DEPLOYMENT_PLATFORM=cloudflare-workers

# 应用配置
NEXT_PUBLIC_APP_URL=http://localhost:8787
NEXT_PUBLIC_APP_NAME="umuo.app - 开发环境"

# AI 服务配置
GROQ_API_KEY=your_groq_api_key_here
TRANSCRIPTION_TIMEOUT_MS=180000
TRANSCRIPTION_RETRY_COUNT=2
TRANSCRIPTION_MAX_CONCURRENCY=2

# 缓存配置
CACHE_TTL=300  # 开发环境更短的缓存时间
ENABLE_CACHE=true

# 分析和监控
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_DEBUG=true

# 数据库配置 (D1)
DATABASE_BINDING="DB"

# 存储配置 (R2)
FILES_BINDING="FILES"

# 缓存配置 (KV)
CACHE_BINDING="CACHE"

# 调试配置
DEBUG=true
LOG_LEVEL="debug"
